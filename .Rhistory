data <- base::readRDS(testthat::test_path("data", "airqplus_pm_copd.rds"))
devtools::load_all(".")
data$incidents_per_100_000_per_year
data$incidents_per_100_000_per_year*8E6/1E5
object_1 =
healthiar::attribute_health(
approach_risk = "relative_risk",
exp_central = data$mean_concentration,
cutoff_central = data$cut_off_value,
bhd_central = data$incidents_per_100_000_per_year/1E5*34000,
rr_central = 1.118,
rr_increment = 10,
erf_shape = "log_linear"
)
object_2 =
healthiar::attribute_health(
approach_risk = "relative_risk",
exp_central = data$mean_concentration-1,
cutoff_central = data$cut_off_value,
bhd_central = data$incidents_per_100_000_per_year/1E5*34000,
rr_central = 1.118,
rr_increment = 10,
erf_shape = "log_linear"
)
healthiar::compare(object_1, object_2)
test <- healthiar::compare(object_1, object_2)
View(test)
View(test[["health_main"]])
